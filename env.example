# Environment Variables for Flex Living Reviews Dashboard
# Copy this file to .env.local for local development

# Application Settings
VITE_APP_TITLE="Flex Living Reviews Dashboard"
VITE_APP_VERSION="1.0.0"

# API Configuration (for future real API integration)
VITE_HOSTAWAY_API_URL="https://api.hostaway.com"
VITE_HOSTAWAY_ACCOUNT_ID="61148"
# Note: Never commit real API keys to version control
VITE_HOSTAWAY_API_KEY="your-api-key-here"

# Feature Flags
VITE_ENABLE_MOCK_DATA="true"
VITE_ENABLE_ANALYTICS="false"

# Google Reviews API (for future Phase 5 implementation)
VITE_GOOGLE_PLACES_API_KEY="your-google-api-key-here"

# Environment
NODE_ENV="production"
